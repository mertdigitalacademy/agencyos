{
  "name": "agencyos",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:api": "tsx watch server/index.ts",
    "dev:all": "concurrently -n api,web -c yellow,cyan \"npm:dev:api\" \"npm:dev\"",
    "dev:netlify": "netlify dev",
    "build:functions": "netlify build",
    "dev:up": "bash scripts/dev-up.sh",
    "dev:down": "bash scripts/dev-down.sh",
    "infra:up": "bash scripts/infra-up.sh",
    "infra:up:all": "bash scripts/infra-up.sh --all",
    "infra:down": "bash scripts/infra-down.sh",
    "documenso:cert": "bash scripts/documenso-cert.sh",
    "selfhost:seed": "bash scripts/selfhost-seed.sh",
    "n8n:up": "bash scripts/n8n-up.sh",
    "n8n:down": "bash scripts/n8n-down.sh",
    "smoke": "bash scripts/smoke-test.sh",
    "db:migrate": "npx tsx scripts/migrate-to-supabase.ts",
    "db:verify": "npx tsx scripts/verify-migration.ts",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"**/*.{ts,tsx,json,md}\""
  },
  "dependencies": {
    "@google/genai": "^1.34.0",
    "@netlify/functions": "^2.8.2",
    "@supabase/supabase-js": "^2.45.0",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^5.1.0",
    "pg": "^8.16.3",
    "react": "^19.2.3",
    "react-dom": "^19.2.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/cors": "^2.8.18",
    "@types/express": "^5.0.3",
    "@types/node": "^22.14.0",
    "@vitejs/plugin-react": "^5.0.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "concurrently": "^9.2.1",
    "eslint": "^9.39.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "jsdom": "^27.0.1",
    "msw": "^2.12.7",
    "prettier": "^3.7.4",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "tsx": "^4.20.5",
    "typescript": "~5.8.2",
    "typescript-eslint": "^8.51.0",
    "vite": "^6.2.0",
    "vitest": "^3.2.4"
  }
}
